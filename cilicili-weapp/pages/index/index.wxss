.van-tabbar {
    background: transparent !important;
    border-top: 1px solid rgba(256, 256, 256, .1);
    padding-top: 5px;
}

.van-tabs__scroll {
    background: transparent !important;
    letter-spacing: 2px !important;
}

.tabbar-add {
    width: 65rpx;
    line-height: 40rpx;
    border-radius: 5px;
    font-size: 50rpx;
    font-weight: 900;
    height: 50rpx;
    text-align: center;
    background: #ffffff;
    margin-bottom: 25rpx;
    border-left: 10rpx solid #16BFFD;
    border-right: 10rpx solid #CB3066;
}

.welcome {
    width: 100%;
    height: 100vh;
    background: #000;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 999;
    font-size: 100rpx;
    text-align: center;
    line-height: 100vh;
}